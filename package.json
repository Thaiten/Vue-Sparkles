{
  "name": "vue-sparkles",
  "version": "1.1.0",
  "description": "A Vue component making things a little sparklier",
  "main": "dist/vue-sparkles.ssr.js",
  "browser": "dist/vue-sparkles.esm.js",
  "module": "dist/vue-sparkles.esm.js",
  "unpkg": "dist/vue-sparkles.min.js",
  "keywords": [
    "effects",
    "vue",
    "vuejs",
    "sparkle",
    "sparkles"
  ],
  "license": "MIT",
  "author": {
    "name": "Eric WÃ¤tke",
    "email": "email@ericwaetke.com",
    "url": "https://ericwaetke.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericwaetke/Vue-Sparkles/"
  },
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "minimist": "^1.2.5",
    "rollup": "^2.46.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=10"
  }
}
